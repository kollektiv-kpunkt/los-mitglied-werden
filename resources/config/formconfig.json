{
  "steps": {
    "welcome": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "welcome.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "welcome.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.member",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "func:s_type_init(member)|goto:name"
            },
            {
              "label": "button.activist",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl los-button-neg",
              "action": "func:s_type_init(volunteer)|goto:name"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "inputs",
      "title": "name.title",
      "fields": [
        {
          "name": "fname",
          "type": "text",
          "label": "fname.label",
          "required": true
        },
        {
          "name": "lname",
          "type": "text",
          "label": "lname.label",
          "required": true
        }
      ],
      "validation": {
        "fname": {
          "presence": true
        },
        "lname": {
          "presence": true
        }
      },
      "next": "email"
    },
    "email": {
      "type": "inputs",
      "title": "email.title",
      "fields": [
        {
          "name": "email",
          "type": "email",
          "size": "-w-full",
          "required": true
        }
      ],
      "validation": {
        "email": {
          "presence": true,
          "email": true
        }
      },
      "next": "phone"
    },
    "phone": {
      "type": "inputs",
      "title": "phone.title",
      "fields": [
        {
          "name": "phone",
          "type": "tel",
          "size": "-w-full",
          "placeholder": "placeholder.optional"
        }
      ],
      "validation": {
        "phone": {
          "format": "^((0041|0)|\\B\\+41)(\\s?\\(0\\))?(\\s)?[1-9]{2}(\\s)?[0-9]{3}(\\s)?[0-9]{2}(\\s)?[0-9]{2}$"
        }
      },
      "next": {
        "type_volunteer": "volunteertype",
        "ELSE": "membertype"
      }
    },
    "membertype": {
      "type": "radios",
      "title": "membertype.title",
      "direction": "vertical",
      "name": "membertype",
      "required": true,
      "choices": [
        {
          "label": "membertype.regular.label",
          "value": "LOS regular"
        },
        {
          "label": "membertype.reduced.label",
          "value": "LOS reduced"
        },
        {
          "label": "membertype.couple.label",
          "value": "LOS couples"
        },
        {
          "label": "membertype.supporter.label",
          "value": "LOS supporter"
        },
        {
          "label": "membertype.organisation.label",
          "value": "LOS organisation"
        }
      ],
      "validation": {
        "membertype": {
          "presence": true
        }
      },
      "next": {
        "membertype_LOS regular": "comboRegular",
        "membertype_LOS reduced": "comboReduced",
        "membertype_LOS organisation": "orgaAddress",
        "ELSE": "address"
      }
    },
    "comboRegular": {
      "type": "radios",
      "title": "comboRegular.title",
      "direction": "horizontal",
      "name": "membertype",
      "choices": [
        {
          "label": "comboRegular.milchjugendU22.label",
          "value": "Combo LOS/Milchjugend u22"
        },
        {
          "label": "comboRegular.milchjugendRegular.label",
          "value": "Combo LOS/Milchjugend regular"
        },
        {
          "label": "comboRegular.hazRegular.label",
          "value": "Combo LOS/HAZ reduced"
        },
        {
          "label": "comboRegular.habsRegular.label",
          "value": "Combo LOS/HABS queer reduced"
        },
        {
          "label": "comboRegular.habRegular.label",
          "value": "Combo LOS/HAB queer regular"
        }
      ],
      "validation": {
        "membertype": {}
      },
      "next": "address"
    },
    "comboReduced": {
      "type": "radios",
      "title": "comboReduced.title",
      "direction": "horizontal",
      "name": "membertype",
      "choices": [
        {
          "label": "comboReduced.milchjugendU22.label",
          "value": "111111111"
        },
        {
          "label": "comboReduced.milchjugendReduced.label",
          "value": "Combo LOS/Milchjugend reduced"
        },
        {
          "label": "comboReduced.hazReduced.label",
          "value": "Combo LOS/HAZ reduced"
        },
        {
          "label": "comboReduced.habsReduced.label",
          "value": "Combo LOS/HABS queer reduced"
        },
        {
          "label": "comboReduced.habReduced.label",
          "value": "Combo LOS/HAB queer reduced"
        }
      ],
      "validation": {
        "membertype": {}
      },
      "next": "address"
    },
    "address": {
      "type": "inputs",
      "title": "address.title",
      "fields": [
        {
          "name": "street",
          "type": "text",
          "label": "street.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "zip",
          "type": "text",
          "label": "zip.label",
          "required": true
        },
        {
          "name": "city",
          "type": "text",
          "label": "city.label",
          "required": true
        }
      ],
      "validation": {
        "street": {
          "presence": true
        },
        "zip": {
          "presence": true
        },
        "city": {
          "presence": true
        }
      },
      "next": "lang"
    },
    "orgaAddress": {
      "type": "inputs",
      "title": "orgaAddress.title",
      "fields": [
        {
          "name": "organame",
          "type": "text",
          "label": "organame.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "street",
          "type": "text",
          "label": "street.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "zip",
          "type": "text",
          "label": "zip.label",
          "required": true
        },
        {
          "name": "city",
          "type": "text",
          "label": "city.label",
          "required": true
        }
      ],
      "validation": {
        "street": {
          "presence": true
        },
        "zip": {
          "presence": true
        },
        "city": {
          "presence": true
        },
        "organame": {
          "presence": true
        }
      },
      "next": "lang"
    },
    "lang": {
      "type": "radios",
      "title": "lang.title",
      "name": "lang",
      "choices": [
        {
          "label": "lang.de.label",
          "value": "German"
        },
        {
          "label": "lang.fr.label",
          "value": "French"
        }
      ],
      "validation": {
        "lang": {
          "presence": true
        }
      },
      "next": "birthday"
    },
    "birthday": {
      "type": "inputs",
      "title": "birthday.title",
      "fields": [
        {
          "name": "birthday",
          "type": "text",
          "placeholder": "dd.mm.yyyy",
          "size": "-w-full"
        }
      ],
      "validation": {
        "birthday": {
          "format": {
            "pattern": "^(0[1-9]|[12][0-9]|3[01])\\.(0[1-9]|1[012])\\.(19|20)\\d\\d$"
          }
        }
      },
      "next": {
        "membertype_LOS couples": "contact2",
        "ELSE": "notes"
      }
    },
    "contact2": {
      "type": "inputs",
      "title": "contact2.title",
      "fields": [
        {
          "name": "fname2",
          "type": "text",
          "label": "fname2.label",
          "required": true
        },
        {
          "name": "lname2",
          "type": "text",
          "label": "lname2.label",
          "required": true
        },
        {
          "name": "email2",
          "type": "email",
          "label": "email2.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "phone2",
          "type": "text",
          "label": "phone2.label",
          "size": "-w-full"
        }
      ],
      "validation": {
        "fname2": {
          "presence": true
        },
        "lname2": {
          "presence": true
        },
        "email2": {
          "presence": true,
          "email": true
        },
        "phone2": {
          "format": "^((0041|0)|\\B\\+41)(\\s?\\(0\\))?(\\s)?[1-9]{2}(\\s)?[0-9]{3}(\\s)?[0-9]{2}(\\s)?[0-9]{2}$"
        }
      },
      "next": "notes"
    },
    "notes": {
      "type": "inputs",
      "title": "notes.title",
      "fields": [
        {
          "name": "notes",
          "placeholder": "placeholder.optional",
          "type": "text",
          "size": "-w-full"
        }
      ],
      "validation": {
        "notes": {}
      },
      "next": "cta"
    },
    "cta": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "cta.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "cta.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.activist",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next los-button-neg text-xl",
              "action": "goto:volunteertype"
            },
            {
              "label": "button.rather-not",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "goto:thanks"
            }
          ]
        }
      ]
    },
    "volunteertype": {
      "type": "radios",
      "title": "volunteertype.title",
      "direction": "vertical",
      "multiple": true,
      "required": true,
      "choices": [
        {
          "label": "homepage.label",
          "value": "homepage"
        },
        {
          "label": "events.label",
          "value": "events"
        },
        {
          "label": "demos.label",
          "value": "demos"
        },
        {
          "label": "lectorate.label",
          "value": "lectorate"
        },
        {
          "label": "translation.label",
          "value": "translation"
        },
        {
          "label": "photography.label",
          "value": "photography"
        },
        {
          "label": "catering.label",
          "value": "catering"
        },
        {
          "label": "barkeeping.label",
          "value": "barkeeping"
        },
        {
          "label": "socialmedia.label",
          "value": "socialmedia"
        },
        {
          "label": "meetingpoints.label",
          "value": "meetingpoints"
        },
        {
          "label": "graphicdesign.label",
          "value": "graphicdesign"
        },
        {
          "label": "other.label",
          "value": "other"
        }
      ],
      "validation": {
        "volunteertype[]": {
          "presence": true
        }
      },
      "next": "locations"
    },
    "locations": {
      "type": "radios",
      "title": "locations.title",
      "direction": "horizontal",
      "multiple": true,
      "choices": [
        {
          "label": "zuerich.label",
          "value": "zuerich"
        },
        {
          "label": "Zurich.label",
          "value": "Zurich"
        },
        {
          "label": "Winterthur.label",
          "value": "Winterthur"
        },
        {
          "label": "StGallen.label",
          "value": "StGallen"
        },
        {
          "label": "Neuchatel.label",
          "value": "Neuchatel"
        },
        {
          "label": "Lucerne.label",
          "value": "Lucerne"
        },
        {
          "label": "Locarno.label",
          "value": "Locarno"
        },
        {
          "label": "Lausanne.label",
          "value": "Lausanne"
        },
        {
          "label": "Geneva.label",
          "value": "Geneva"
        },
        {
          "label": "Berne.label",
          "value": "Berne"
        },
        {
          "label": "Basel.label",
          "value": "Basel"
        }
      ],
      "validation": {
        "locations[]": {}
      },
      "next": "thanks"
    },
    "thanks": {
      "type": "contentblocks",
      "finished": true,
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "thanks.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "thanks.text",
          "class": "text-white text-2xl mb-6"
        }
      ]
    }
  }
}