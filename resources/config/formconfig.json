{
  "steps": {
    "welcome": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "welcome.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "welcome.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.member",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "func:s_type_init(member)|goto:name"
            },
            {
              "label": "button.activist",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl los-button-neg",
              "action": "func:s_type_init(volunteer)|goto:name"
            }
          ]
        }
      ]
    },
    "name": {
      "type": "inputs",
      "title": "name.title",
      "fields": [
        {
          "name": "fname",
          "type": "text",
          "label": "fname.label",
          "required": true
        },
        {
          "name": "lname",
          "type": "text",
          "label": "lname.label",
          "required": true
        }
      ],
      "validation": {
        "fname": {
          "presence": true
        },
        "lname": {
          "presence": true
        }
      },
      "next": "email"
    },
    "email": {
      "type": "inputs",
      "title": "email.title",
      "fields": [
        {
          "name": "email",
          "type": "email",
          "size": "-w-full",
          "required": true
        }
      ],
      "validation": {
        "email": {
          "presence": true,
          "email": true
        }
      },
      "next": "phone"
    },
    "phone": {
      "type": "inputs",
      "title": "phone.title",
      "fields": [
        {
          "name": "phone",
          "type": "tel",
          "size": "-w-full"
        }
      ],
      "validation": {
        "phone": {
          "format": "^((0041|0)|\\B\\+41)(\\s?\\(0\\))?(\\s)?[1-9]{2}(\\s)?[0-9]{3}(\\s)?[0-9]{2}(\\s)?[0-9]{2}$"
        }
      },
      "next": {
        "type_volunteer": "volunteertype",
        "ELSE": "membertype"
      }
    },
    "membertype": {
      "type": "radios",
      "title": "membertype.title",
      "direction": "vertical",
      "name": "membertype",
      "required": true,
      "choices": [
        {
          "label": "membertype.regular.label",
          "value": "LOS regular"
        },
        {
          "label": "membertype.reduced.label",
          "value": "LOS reduced"
        },
        {
          "label": "membertype.couple.label",
          "value": "LOS couples"
        },
        {
          "label": "membertype.organisation.label",
          "value": "LOS organisation"
        },
        {
          "label": "membertype.supporter.label",
          "value": "LOS supporter"
        }
      ],
      "validation": {
        "membertype": {
          "presence": true
        }
      },
      "next": {
        "membertype_LOS regular": "comboRegular_optin",
        "membertype_LOS reduced": "comboReduced_optin",
        "membertype_LOS organisation": "orgaAddress",
        "ELSE": "address"
      }
    },
    "comboRegular_optin": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "combo.optin.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "combo.optin.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.yes-please",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next los-button-neg text-xl",
              "action": "goto:comboRegular"
            },
            {
              "label": "button.rather-not",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "func:s_update_field(membertype, LOS regular)|goto:address"
            }
          ]
        }
      ]
    },
    "comboReduced_optin": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "combo.optin.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "combo.optin.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.yes-please",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next los-button-neg text-xl",
              "action": "goto:comboReduced"
            },
            {
              "label": "button.rather-not",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "goto:address"
            }
          ]
        }
      ]
    },
    "comboRegular": {
      "type": "radios",
      "title": "combo.title",
      "direction": "horizontal",
      "name": "membertype",
      "choices": [
        {
          "label": "combo.milchjugendU22.label",
          "value": "Combo LOS/Milchjugend u22"
        },
        {
          "label": "comboRegular.milchjugend.label",
          "value": "Combo LOS/Milchjugend regular"
        },
        {
          "label": "comboRegular.haz.label",
          "value": "Combo LOS/HAZ regular"
        },
        {
          "label": "comboRegular.habs.label",
          "value": "Combo LOS/HABS queer regular"
        },
        {
          "label": "comboRegular.hab.label",
          "value": "Combo LOS/HAB queer regular"
        }
      ],
      "validation": {
        "membertype": {}
      },
      "next": "address"
    },
    "comboReduced": {
      "type": "radios",
      "title": "combo.title",
      "direction": "horizontal",
      "name": "membertype",
      "choices": [
        {
          "label": "combo.milchjugendU22.label",
          "value": "Combo LOS/Milchjugend u22"
        },
        {
          "label": "comboReduced.milchjugend.label",
          "value": "Combo LOS/Milchjugend reduced"
        },
        {
          "label": "comboReduced.haz.label",
          "value": "Combo LOS/HAZ reduced"
        },
        {
          "label": "comboReduced.habs.label",
          "value": "Combo LOS/HABS queer reduced"
        },
        {
          "label": "comboReduced.hab.label",
          "value": "Combo LOS/HAB queer reduced"
        }
      ],
      "validation": {
        "membertype": {}
      },
      "next": "address"
    },
    "address": {
      "type": "inputs",
      "title": "address.title",
      "fields": [
        {
          "name": "street",
          "type": "text",
          "label": "street.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "zip",
          "type": "text",
          "label": "zip.label",
          "required": true
        },
        {
          "name": "city",
          "type": "text",
          "label": "city.label",
          "required": true
        }
      ],
      "validation": {
        "street": {
          "presence": true
        },
        "zip": {
          "presence": true
        },
        "city": {
          "presence": true
        }
      },
      "next": "lang"
    },
    "orgaAddress": {
      "type": "inputs",
      "title": "orgaAddress.title",
      "fields": [
        {
          "name": "organame",
          "type": "text",
          "label": "organame.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "street",
          "type": "text",
          "label": "street.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "zip",
          "type": "text",
          "label": "zip.label",
          "required": true
        },
        {
          "name": "city",
          "type": "text",
          "label": "city.label",
          "required": true
        }
      ],
      "validation": {
        "street": {
          "presence": true
        },
        "zip": {
          "presence": true
        },
        "city": {
          "presence": true
        },
        "organame": {
          "presence": true
        }
      },
      "next": "lang"
    },
    "lang": {
      "type": "radios",
      "title": "lang.title",
      "name": "lang",
      "choices": [
        {
          "label": "lang.de.label",
          "value": "DE"
        },
        {
          "label": "lang.fr.label",
          "value": "FR"
        }
      ],
      "validation": {
        "lang": {
          "presence": true
        }
      },
      "next": {
        "membertype_Combo LOS/Milchjugend u22": "birthyear_required",
        "ELSE": "birthyear"
      }
    },
    "birthyear": {
      "type": "inputs",
      "title": "birthyear.title",
      "fields": [
        {
          "name": "birthyear",
          "placeholder": "placeholder.optional",
          "type": "number",
          "size": "-w-full"
        }
      ],
      "validation": {
        "birthyear": {}
      },
      "next": {
        "membertype_LOS couples": "contact2",
        "ELSE": "notes"
      }
    },
    "birthyear_required": {
      "type": "inputs",
      "title": "birthyear.title",
      "fields": [
        {
          "name": "birthyear",
          "type": "number",
          "size": "-w-full"
        }
      ],
      "validation": {
        "birthyear": {
          "presence": true
        }
      },
      "next": {
        "membertype_LOS couples": "contact2",
        "ELSE": "notes"
      }
    },
    "contact2": {
      "type": "inputs",
      "title": "contact2.title",
      "fields": [
        {
          "name": "fname2",
          "type": "text",
          "label": "fname2.label",
          "required": true
        },
        {
          "name": "lname2",
          "type": "text",
          "label": "lname2.label",
          "required": true
        },
        {
          "name": "email2",
          "type": "email",
          "label": "email2.label",
          "required": true,
          "size": "-w-full"
        },
        {
          "name": "phone2",
          "type": "text",
          "label": "phone2.label",
          "size": "-w-full"
        }
      ],
      "validation": {
        "fname2": {
          "presence": true
        },
        "lname2": {
          "presence": true
        },
        "email2": {
          "presence": true,
          "email": true
        },
        "phone2": {
          "format": "^((0041|0)|\\B\\+41)(\\s?\\(0\\))?(\\s)?[1-9]{2}(\\s)?[0-9]{3}(\\s)?[0-9]{2}(\\s)?[0-9]{2}$"
        }
      },
      "next": "notes"
    },
    "notes": {
      "type": "inputs",
      "title": "notes.title",
      "fields": [
        {
          "name": "notes",
          "placeholder": "placeholder.optional",
          "type": "text",
          "size": "-w-full"
        }
      ],
      "validation": {
        "notes": {}
      },
      "next": "cta"
    },
    "cta": {
      "type": "contentblocks",
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "cta.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "cta.text",
          "class": "text-white text-2xl mb-6"
        },
        {
          "type": "buttons",
          "buttons": [
            {
              "label": "button.activist",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next los-button-neg text-xl",
              "action": "goto:volunteertype"
            },
            {
              "label": "button.rather-not",
              "markup": "button",
              "type": "los-memberform",
              "class": "los-button-next text-xl",
              "action": "goto:thanks"
            }
          ]
        }
      ]
    },
    "volunteertype": {
      "type": "radios",
      "title": "volunteertype.title",
      "direction": "vertical",
      "multiple": true,
      "required": true,
      "choices": [
        {
          "label": "Events.label",
          "value": "Events"
        },
        {
          "label": "Demonstrations & Prides.label",
          "value": "Demonstrations & Prides"
        },
        {
          "label": "Website (Wordpress).label",
          "value": "Website (Wordpress)"
        },
        {
          "label": "Proof Reading.label",
          "value": "Proof Reading"
        },
        {
          "label": "Translations.label",
          "value": "Translations"
        },
        {
          "label": "Photography.label",
          "value": "Photography"
        },
        {
          "label": "Catering.label",
          "value": "Catering"
        },
        {
          "label": "Bar.label",
          "value": "Bar"
        },
        {
          "label": "Social Media.label",
          "value": "Social Media"
        },
        {
          "label": "Graphic Design.label",
          "value": "Graphic Design"
        },
        {
          "label": "Driving.label",
          "value": "Driving"
        },
        {
          "label": "Other.label",
          "value": "Other"
        }
      ],
      "validation": {
        "volunteertype[]": {
          "presence": true
        }
      },
      "next": "locations"
    },
    "locations": {
      "type": "radios",
      "title": "locations.title",
      "direction": "horizontal",
      "multiple": true,
      "choices": [
        {
          "label": "Basel.label",
          "value": "Basel"
        },
        {
          "label": "Bern.label",
          "value": "Berne"
        },
        {
          "label": "Geneva.label",
          "value": "Geneva"
        },
        {
          "label": "Lausanne.label",
          "value": "Lausanne"
        },
        {
          "label": "Locarno.label",
          "value": "Locarno"
        },
        {
          "label": "Lucerne.label",
          "value": "Lucerne"
        },
        {
          "label": "Neuchatel.label",
          "value": "Neuchatel"
        },
        {
          "label": "St. Gallen.label",
          "value": "StGallen"
        },
        {
          "label": "Winterthur.label",
          "value": "Winterthur"
        },
        {
          "label": "Zurich.label",
          "value": "Zurich"
        }
      ],
      "validation": {
        "locations[]": {}
      },
      "next": "thanks"
    },
    "thanks": {
      "type": "contentblocks",
      "finished": true,
      "blocks": [
        {
          "type": "title",
          "size": "h1",
          "content": "thanks.title",
          "class": "text-white text-6xl font-bold mb-6"
        },
        {
          "type": "text",
          "content": "thanks.text",
          "class": "text-white text-2xl mb-6"
        }
      ]
    }
  }
}